doctype html
html
  head
    meta(charset="UTF-8")
    link(rel="icon", href="/images/favicon.png")
    link(rel="stylesheet", href="/stylesheets/w3.css")
    title Student Registered
  body
    .w3-card-4
      header.w3-container.w3-teal
        h1 Student Registered Successfully
      .w3-container
        p
          b Id:
          | #{aluno.id}
        p
          b Name:
          | #{aluno.nome}
        p
          b Git:
          | #{aluno.git}
        p
          b TPCs:
        ul
          each tpc, index in Object.keys(aluno).filter(key => key.startsWith('tpc'))
            li TPC #{index + 1}: #{aluno[tpc] ? 'Completed' : 'Not Completed'}
      footer.w3-container.w3-teal
        h5 Generated by RPCW2023 in #{d}
        a.w3-btn.w3-round.w3-grey(href="/") Back